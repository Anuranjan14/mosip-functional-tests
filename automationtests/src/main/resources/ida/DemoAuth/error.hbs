{
  "authResponse": {
    "body": {
    "errors": [
      {{#each errors}}
        {
            "errorCode": "{{errorCode}}",
            "errorMessage": "{{errorMessage}}"
        }
          {{#unless @last}},{{/unless}}
      	{{/each}}
  ]
      "response": {
        "authStatus": {{authStatus}}
      }
    }
  }
}